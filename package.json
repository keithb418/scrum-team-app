    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.6",
